al\n            })\n            \n        case (M_R):\n            return ({\n                ...state,\n                total: state.memory\n            })\n        case (M_C):\n            return ({\n                ...state,\n                memory: 0\n            })\n        default:\n            return state;\n    }\n}\n\nexport default reducer;","/Users/chrisjramirez/Documents/Unit3Flex-Lambda/web-module-project-reducer/src/actions/index.js",[],{"ruleId":"39","severity":1,"message":"40","line":1,"column":16,"nodeType":"41","messageId":"42","endLine":1,"endColumn":24},{"ruleId":"39","severity":1,"message":"43","line":9,"column":9,"nodeType":"41","messageId":"42","endLine":9,"endColumn":15},{"ruleId":"44","severity":1,"message":"45","line":26,"column":9,"nodeType":"46","endLine":26,"endColumn":46},{"ruleId":"47","severity":1,"message":"48","line":26,"column":46,"nodeType":"46","endLine":26,"endColumn":93},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","severity":1,"message":"54","line":11,"column":5,"nodeType":"55","messageId":"56","endLine":18,"endColumn":6},"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'addOne' is defined but never used.","jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-is-valid.md","JSXOpeningElement","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","no-native-reassign",["57"],"no-negated-in-lhs",["58"],"default-case","Expected a default case.","SwitchStatement","missingDefaultCase","no-global-assign","no-unsafe-negation"]